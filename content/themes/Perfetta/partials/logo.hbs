<header id="logo" class="page-wrap">
    {{#if @blog.logo}}
    	<a id="logo-image" href="{{@blog.url}}">
    		<img src="{{@blog.logo}}" alt="{{@blog.title}} - {{@blog.description}}" />
    	</a>
    {{else}}
	    <h1 class="blog-title">
	    	<a href="{{@blog.url}}">{{@blog.title}}</a>
	    </h1>
	    <h2 class="blog-desc">
	    	<a href="{{@blog.url}}">{{@blog.description}}</a>
	    </h2>

	    <ul>
	    	<li class="first"><a href="">Us</a></li>
	    	<li><a href="">The day</a></li>
	    	<li><a href="">Location</a></li>
	    	<li><a href="">Accommodation</a></li>
	    	<li><a href="">Gift</a> List</li>
	    	<li><a href="">Contact</a></li>
	    </ul>
    {{/if}}
</header>